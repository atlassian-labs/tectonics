{
	"name": "@atlassian/tectonics",
	"version": "0.0.1",
	"description": "Atlaskit but with Web Components",
	"main": "dist/index.cjs.js",
	"module": "dist/custom-elements/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/custom-elements/index.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/tectonics/tectonics.esm.js",
	"files": [
		"dist/",
		"loader/"
	],
	"scripts": {
		"clean": "rimraf loader dist package public www",
		"lint": "eslint src --ext .ts,.tsx",
		"build": "stencil build --docs",
		"start": "run-p start:*",
		"start:stencil": "stencil build --dev --watch --serve",
		"start:storybook": "start-storybook -p 7002 -s www -c .storybook --ci",
		"test": "stencil test --spec --e2e",
		"test:watch": "stencil test --spec --e2e --watchAll",
		"generate": "stencil generate",
		"build:storybook": "build-storybook -c ./.storybook -o ../public",
		"storybook": "run-p build start:storybook"
	},
	"dependencies": {
		"@stencil/core": "^2.7.0",
		"@stencil/sass": "^1.4.1",
		"clsx": "^1.1.1"
	},
	"devDependencies": {
		"@stencil/postcss": "^2.0.0",
		"@storybook/addon-a11y": "^6.3.8",
		"@storybook/addon-actions": "^6.3.8",
		"@storybook/addon-backgrounds": "^6.3.8",
		"@storybook/addon-essentials": "^6.3.8",
		"@storybook/addon-jest": "^6.3.8",
		"@storybook/addon-knobs": "^6.3.1",
		"@storybook/addon-links": "^6.3.8",
		"@storybook/addon-notes": "^5.3.21",
		"@storybook/addon-options": "^5.3.21",
		"@storybook/addon-viewport": "^6.3.8",
		"@storybook/html": "^6.3.8",
		"@storybook/preset-typescript": "^3.0.0",
		"@types/jest": "^26.0.24",
		"@typescript-eslint/eslint-plugin": "^4.30.0",
		"eslint": "^7.32.0",
		"eslint-plugin-jest": "^24.4.0",
		"jest": "^26.6.3",
		"jest-cli": "^26.6.3",
		"jest-junit": "^12.2.0",
		"jest-transform-stub": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^10.0.0",
		"rimraf": "^3.0.2",
		"sass-loader": "^12.1.0",
		"stencil-inline-svg": "^1.1.0"
	},
	"license": "GPL.v3"
}
